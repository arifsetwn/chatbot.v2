# Streamlit Secrets Configuration Example
# Copy this to .streamlit/secrets.toml and fill in your actual values
# In Streamlit Cloud, add these in the app settings under "Secrets"

# API Keys
GEMINI_API_KEY = "your_gemini_api_key_here"
GOOGLE_API_KEY = "your_google_api_key_here"
OPENAI_API_KEY = "your_openai_api_key_here"

# Application Settings
APP_NAME = "Chatbot Pembelajaran Algoritma"
APP_VERSION = "1.0.0"

# Security Settings
SECRET_KEY = "your_secret_key_here"
ADMIN_USERNAME = "admin"
ADMIN_PASSWORD = "admin123"

# Rate Limiting
RATE_LIMIT_PER_MINUTE = 10
RATE_LIMIT_PER_HOUR = 100
GLOBAL_RATE_LIMIT = 60

# Model Settings
ACTIVE_MODEL = "gemini"
DEFAULT_MODEL = "gemini"
GEMINI_MODEL = "gemini-2.0-flash"
